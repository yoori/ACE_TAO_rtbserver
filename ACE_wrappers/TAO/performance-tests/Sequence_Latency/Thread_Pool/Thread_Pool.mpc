project(*sequence_idl): taoidldefaults, strategies {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*sequence server): taoserver, strategies {
  after += *sequence_idl
  Source_Files {
    TestC.cpp
    TestS.cpp
    Roundtrip.cpp
    Worker_Thread.cpp
    server.cpp
  }
