project(*idl): taoidldefaults {
  IDL_Files {
    time.idl
  }
  custom_only = 1
}

project(*Server): taoserver {
  after += *idl
  Source_Files {
    server.cpp
  }
  Source_Files {
    timeC.cpp
    timeS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoclient, anytypecode {
  after += *idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    timeC.cpp
  }
  IDL_Files {
  }
}

