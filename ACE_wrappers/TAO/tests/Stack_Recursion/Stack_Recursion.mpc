project(*idl): taoidldefaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*Server): taoserver {
  after += *idl
  Source_Files {
    server.cpp
    Server_Task.cpp
    Sender.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

project(*Client): taoclient, anytypecode {
  after += *idl
  Source_Files {
    client.cpp
    Client_Task.cpp
  }
  Source_Files {
    TestC.cpp
  }
  IDL_Files {
  }
}

